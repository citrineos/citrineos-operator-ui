{
  "name": "citrineos-operator-ui",
  "version": "1.1.1",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "clean-tsbuildinfo": "find . -name tsconfig.tsbuildinfo -not -path '*/node_modules/*' -exec rm -f {} +",
    "clean-dist": "find . -type d -name .next -not -path '*/node_modules/*' -exec rm -rf {} +",
    "clean-cache": "npm cache clean --force",
    "clean": "npm run clean-dist && npm run clean-tsbuildinfo && npm run clean-cache",
    "remove-dependencies": "rm -rf package-lock.json **/package-lock.json **/**/package-lock.json node_modules **/node_modules **/**/node_modules",
    "fresh": "npm run clean && npm run remove-dependencies",
    "fi": "npm run fresh && npm install",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 refine dev",
    "build": "refine build",
    "start": "refine start",
    "prettier": "prettier --write .",
    "lint": "npx eslint ./",
    "lint-fix": "npm run prettier && npx eslint --fix ./",
    "refine": "refine"
  },
